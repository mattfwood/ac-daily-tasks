{
  "name": "ac-daily-tasks",
  "version": "1.0.0",
  "scripts": {
    "dev": "blitz start",
    "build": "blitz db migrate && blitz build",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx .",
    "test": "echo \"No tests yet\""
  },
  "browserslist": [
    "defaults"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && pretty-quick --staged",
      "pre-push": "blitz test"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@prisma/cli": "2.0.0-beta.3",
    "@prisma/client": "2.0.0-beta.3",
    "@reach/accordion": "0.10.1",
    "@reach/checkbox": "0.10.1",
    "@reach/tabs": "0.10.1",
    "@sentry/browser": "^5.1.0",
    "@sentry/node": "^5.6.2",
    "@sentry/webpack-plugin": "1.11.1",
    "@zeit/next-source-maps": "0.0.4-canary.1",
    "axios": "0.19.2",
    "blitz": "latest",
    "cookie": "0.4.1",
    "date-fns": "2.12.0",
    "eslint-config-cratebind": "5.0.2",
    "js-cookie": "2.2.1",
    "jsonwebtoken": "8.5.1",
    "minerva-ui": "3.4.5",
    "nodemailer": "6.4.6",
    "postmark": "2.5.1",
    "react": "experimental",
    "react-dom": "experimental",
    "styled-components": "5.1.0",
    "typescript": "3.8.3"
  },
  "NOTE": "Next.js dependency is currently only required for deploying to zeit",
  "devDependencies": {
    "@types/react": "16.9.34",
    "@typescript-eslint/eslint-plugin": "2.29.0",
    "@typescript-eslint/parser": "2.29.0",
    "babel-eslint": "10.1.0",
    "eslint": "6.8.0",
    "eslint-config-react-app": "5.2.1",
    "eslint-plugin-flowtype": "4.7.0",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.19.0",
    "eslint-plugin-react-hooks": "2.5.1",
    "husky": "4.2.5",
    "lint-staged": "10.1.7",
    "next": "9.3.5",
    "prettier": "2.0.5",
    "pretty-quick": "2.0.1"
  },
  "private": true
}
